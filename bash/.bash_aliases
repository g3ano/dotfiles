# Aliases
# Tools

alias xamppst="sudo /opt/lampp/lampp start"
alias xamppsp="sudo /opt/lampp/lampp stop"
alias xamppss="sudo /opt/lampp/lampp status"
alias xampprt="sudo /opt/lampp/lampp restart"

alias vim="nvim"

alias zed="flatpak run dev.zed.Zed"

# Directories
alias dirconfig="cd ~/.config"
alias dirworkspace="cd ~/workspace"
alias dirhtdocs="cd /opt/lampp/htdocs"
alias dirlaravel="cd /opt/lampp/htdocs/laravel"
alias dirapis="cd /opt/lampp/htdocs/apis"
alias dirsites="cd ~/workspace/sites"

