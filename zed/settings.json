{
	"icon_theme": "Catppuccin Frappé",
	"theme": "Catppuccin Frappé",
	"edit_predictions": {
		"mode": "eager",
		"copilot": {
			"proxy": null,
			"proxy_no_verify": null
		},
		"enabled_in_assistant": false
	},
	"ui_font_family": "Hack",
	"ui_font_size": 16,
	"buffer_font_family": "Hack",
	"buffer_font_size": 16,
	"soft_wrap": "editor_width",
	"base_keymap": "VSCode",
	"max_tabs": 20,
	"extend_comment_on_newline": true,
	"vim_mode": true,
	"autosave": "on_window_change",
	"cursor_blink": false,
	"completion_documentation_secondary_query_debounce": 500,
	"vertical_scroll_margin": 10,
	"scroll_beyond_last_line": "vertical_scroll_margin",
	"use_smartcase_search": true,
	"confirm_quit": true,
	"expand_excerpt_lines": 20,
	"show_whitespaces": "none",
	"use_autoclose": true,
	"show_completions_on_input": true,
	"relative_line_numbers": true,
	"hard_tabs": true,
	"tab_size": 4,
	"auto_signature_help": false,
	"current_line_highlight": "all",
	"show_edit_predictions": true,
	"ensure_final_newline_on_save": false,
	"format_on_save": "on",
	"use_on_type_format": false,
	"use_system_path_prompts": false,
	"show_wrap_guides": false,
	"preview_tabs": {
		"enabled": false
	},
	"assistant": {
		"default_model": {
			"provider": "zed.dev",
			"model": "claude-3-5-sonnet-latest"
		},
		"version": "2",
		"default_width": 470
	},
	"features": {
		"edit_prediction_provider": "zed"
	},
	"tab_bar": {
		"show": false,
		"show_nav_history_buttons": false,
		"show_tab_bar_buttons": false
	},
	"tabs": {
		"close_position": "right",
		"file_icons": true,
		"git_status": true
	},
	"file_finder": {
		"modal_max_width": "medium"
	},
	"file_scan_exclusions": [
		"**/.git",
		"**/.svn",
		"**/.hg",
		"**/CVS",
		"**/.DS_Store",
		"**/Thumbs.db",
		"**/.classpath",
		"**/.settings",
		"**/out",
		"**/dist",
		"**/.husky",
		"**/.turbo",
		"**/.vscode-test",
		"**/.vscode",
		"**/.next",
		"**/.storybook",
		"**/.tap",
		"**/.nyc_output",
		"**/report",
		"**/node_modules",
		"**/vendor/**"
	],
	"active_pane_modifiers": {
		"border_size": 1,
		"inactive_opacity": 2
	},
	"scrollbar": {
		"show": "system",
		"axes": {
			"horizontal": false,
			"vertical": true
		}
	},
	"toolbar": {
		"breadcrumbs": true,
		"quick_actions": true,
		"selections_menu": false
	},
	"collaboration_panel": {
		"default_width": 280,
		"button": false
	},
	"project_panel": {
		"scrollbar": {
			"show": "never"
		},
		"indent_guides": {
			"show": "never"
		},
		"auto_fold_dirs": false,
		"auto_reveal_entries": true,
		"default_width": 360,
		"folder_icons": false,
		"file_icons": true,
		"indent_size": 20
	},
	"git_panel": {
		"default_width": 360
	},
	"outline_panel": {
		"default_width": 360
	},
	"chat_panel": {
		"button": "never"
	},
	"telemetry": {
		"diagnostics": false,
		"metrics": false
	},
	"search": {
		"case_sensitive": false,
		"include_ignored": false
	},
	"indent_guides": {
		"enabled": false
	},
	"inlay_hints": {
		"enabled": false
	},
	"gutter": {
		"line_numbers": true,
		"code_actions": false,
		"folds": true,
		"runnables": true
	},
	"vim": {
		"use_system_clipboard": "always",
		"use_smartcase_find": true,
		"use_multiline_find": false
	},
	"file_types": {
		"Dockerfile": ["Dockerfile", "Dockerfile.*"],
		"JSON": ["json", "jsonc", "*.code-snippets"]
	},
	"languages": {
		"Go": {
			"show_edit_predictions": true
		},
		"PHP": {
			"language_servers": ["intelephense", "!phpactor", "..."]
		},
		"TSX": {
			"language_servers": ["!typescript-language-server", "vtsls", "..."],
			"tab_size": 2,
			"formatter": {
				"external": {
					"command": "prettier",
					"arguments": ["--stdin-filepath", "{buffer_path}"]
				}
			},
			"code_actions_on_format": {
				"source.fixAll.eslint": true,
				"source.organizeImports": true
			}
		},
		"TypeScript": {
			"language_servers": ["!typescript-language-server", "vtsls", "..."],
			"tab_size": 2,
			"formatter": {
				"external": {
					"command": "prettier",
					"arguments": ["--stdin-filepath", "{buffer_path}"]
				}
			},
			"code_actions_on_format": {
				"source.fixAll.eslint": true,
				"source.organizeImports": true
			}
		},
		"JSON": {
			"tab_size": 2,
			"hard_tabs": true
		},
		"JSONC": {
			"tab_size": 2,
			"hard_tabs": true
		},
		"YAML": {
			"tab_size": 2,
			"hard_tabs": false
		},
		"CSS": {
			"tab_size": 2,
			"hard_tabs": true
		}
	},
	"lsp": {
		"vtsls": {
			"settings": {
				"typescript": {
					"tsserver": { "maxTsServerMemory": 1024 },
					"inlay_hints": {
						"enabled": true,
						"show_parameter_hints": false,
						"show_other_hints": true,
						"show_type_hints": true
					},
					"preferences": {
						"importModuleSpecifier": "non-relative"
					}
				}
			}
		},
		"tailwindcss-language-server": {
			"settings": {
				"classAttributes": ["class", "className", "ngClass", "styles"]
			}
		}
	},
	"terminal": {
		"env": {
			"EDITOR": "zed --wait"
		}
	},
	"node": {
		"path": "/home/g3ano/.nvm/versions/node/v20.18.1/bin/node",
		"npm_path": "/home/g3ano/.nvm/versions/node/v20.18.1/bin/npm"
	}
}
