{
	"icon_theme": "Catppuccin Frapp√©",
	"theme": "One Dark",
	"ui_font_family": "Adwaita Mono",
	"ui_font_size": 16,
	"buffer_font_family": "Adwaita Mono",
	"buffer_font_size": 15.5,
	"soft_wrap": "editor_width",
	"base_keymap": "VSCode",
	"max_tabs": 30,
	"extend_comment_on_newline": true,
	"autosave": "on_window_change",
	"cursor_blink": false,
	"show_completions_on_input": true,
	"vertical_scroll_margin": 10,
	"scroll_beyond_last_line": "vertical_scroll_margin",
	"use_smartcase_search": true,
	"confirm_quit": true,
	"expand_excerpt_lines": 20,
	"show_whitespaces": "none",
	"use_autoclose": true,
	"relative_line_numbers": true,
	"hard_tabs": true,
	"tab_size": 4,
	"auto_signature_help": false,
	"current_line_highlight": "all",
	"show_edit_predictions": true,
	"ensure_final_newline_on_save": false,
	"format_on_save": "on",
	"use_on_type_format": false,
	"use_system_path_prompts": false,
	"show_wrap_guides": false,
	"seed_search_query_from_cursor": "selection",
	"vim_mode": true,
	"minimap": {
		"show": "always",
		"current_line_highlight": null,
		"thumb_border": "none"
	},
	"completions": {
		"lsp": true,
		"lsp_fetch_timeout_ms": 0,
		"lsp_insert_mode": "replace_subsequence"
	},
	"preview_tabs": {
		"enabled": false
	},
	"agent": {
		"single_file_review": false,
		"default_profile": "minimal",
		"default_model": {
			"provider": "zed.dev",
			"model": "claude-sonnet-4"
		},
		"default_width": 400,
		"enabled": false
	},
	"features": {
		"edit_prediction_provider": "supermaven"
	},
	"edit_predictions": {
		"mode": "eager"
	},
	"tab_bar": {
		"show": false,
		"show_nav_history_buttons": false,
		"show_tab_bar_buttons": false
	},
	"tabs": {
		"close_position": "right",
		"file_icons": true,
		"git_status": true
	},
	"file_finder": {
		"modal_max_width": "medium"
	},
	"file_scan_exclusions": [
		"**/.git",
		"**/.svn",
		"**/.hg",
		"**/CVS",
		"**/.DS_Store",
		"**/Thumbs.db",
		"**/.classpath",
		"**/.settings",
		"**/out",
		"**/dist",
		"**/.husky",
		"**/.turbo",
		"**/.vscode-test",
		"**/.vscode",
		"**/.next",
		"**/.storybook",
		"**/.tap",
		"**/.nyc_output",
		"**/report",
		"**/node_modules",
		"**/vendor/**"
	],
	"active_pane_modifiers": {
		"border_size": 1,
		"inactive_opacity": 2
	},
	"scrollbar": {
		"show": "never",
		"axes": {
			"horizontal": false,
			"vertical": true
		}
	},
	"toolbar": {
		"breadcrumbs": true,
		"quick_actions": true,
		"selections_menu": false,
		"agent_review": false
	},
	"collaboration_panel": {
		"default_width": 400,
		"button": false
	},
	"project_panel": {
		"scrollbar": {
			"show": "never"
		},
		"indent_guides": {
			"show": "never"
		},
		"default_width": 400,
		"auto_fold_dirs": false,
		"auto_reveal_entries": true,
		"folder_icons": false,
		"file_icons": true,
		"indent_size": 20,
		"hide_gitignore": false,
		"entry_spacing": "comfortable",
		"sticky_scroll": false,
		"git_status": true,
		"hide_root": false,
		"show_diagnostics": "all"
	},
	"git_panel": {
		"default_width": 400,
		"status_style": "icon",
		"button": true,
		"dock": "left",
		"scrollbar": {
			"show": "never"
		}
	},
	"outline_panel": {
		"default_width": 400
	},
	"chat_panel": {
		"default_width": 400,
		"button": "never"
	},
	"telemetry": {
		"diagnostics": false,
		"metrics": false
	},
	"title_bar": {
		"show_branch_icon": true,
		"show_branch_name": true,
		"show_onboarding_banner": true,
		"show_project_items": true
	},
	"search": {
		"case_sensitive": false,
		"include_ignored": false,
		"button": false
	},
	"debugger": {
		"button": false
	},
	"indent_guides": {
		"enabled": false
	},
	"inlay_hints": {
		"enabled": false
	},
	"gutter": {
		"line_numbers": true,
		"folds": true,
		"runnables": true,
		"breakpoints": false
	},
	"vim": {
		"use_system_clipboard": "always",
		"use_smartcase_find": true
	},
	"file_types": {
		"Dockerfile": ["Dockerfile", "Dockerfile.*"],
		"JSON": ["json", "jsonc", "*.code-snippets"]
	},
	"languages": {
		"Go": {
			"show_edit_predictions": true
		},
		"PHP": {
			"language_servers": ["intelephense", "!phpactor", "..."]
		},
		"TSX": {
			"language_servers": ["!typescript-language-server", "vtsls", "..."],
			"tab_size": 2,
			"formatter": {
				"external": {
					"command": "prettier",
					"arguments": ["--stdin-filepath", "{buffer_path}"]
				}
			},
			"code_actions_on_format": {
				"source.fixAll.eslint": true,
				"source.organizeImports": true
			}
		},
		"TypeScript": {
			"language_servers": ["!typescript-language-server", "vtsls", "..."],
			"tab_size": 2,
			"formatter": {
				"external": {
					"command": "prettier",
					"arguments": ["--stdin-filepath", "{buffer_path}"]
				}
			},
			"code_actions_on_format": {
				"source.fixAll.eslint": true,
				"source.organizeImports": true
			}
		},
		"JSON": {
			"tab_size": 2,
			"hard_tabs": true
		},
		"JSONC": {
			"tab_size": 2,
			"hard_tabs": true
		},
		"YAML": {
			"tab_size": 2,
			"hard_tabs": false
		},
		"CSS": {
			"tab_size": 2,
			"hard_tabs": true
		},
		"Templ": {
			"formatter": {
				"external": {
					"command": "templ",
					"arguments": ["fmt", "-stdin-filepath", "{buffer_path}"]
				}
			},
			"format_on_save": "on",
			"language_servers": [
				"tailwindcss-language-server",
				"vscode-html-language-server",
				"emmet-language-server"
			],
			"inlay_hints": {
				"enabled": true
			}
		}
	},
	"lsp": {
		"vtsls": {
			"settings": {
				"typescript": {
					"tsserver": { "maxTsServerMemory": 512 },
					"inlay_hints": {
						"enabled": true,
						"show_parameter_hints": false,
						"show_other_hints": true,
						"show_type_hints": true
					},
					"preferences": {
						"importModuleSpecifier": "non-relative"
					}
				}
			}
		},
		"tailwindcss-language-server": {
			"settings": {
				"classAttributes": ["class", "className", "ngClass", "styles"],
				"includeLanguages": {
					"templ": "html",
					"Templ": "html"
				}
			}
		},
		"vscode-html-language-server": {
			"settings": {
				"html": {
					"suggest": {
						"html5": true,
						"angular1": true,
						"ionic": true
					},
					"validate": {
						"scripts": true,
						"styles": true
					},
					"format": {
						"wrapLineLength": 120,
						"unformatted": "wbr,code,pre"
					},
					"includeLanguages": {
						"templ": "html",
						"Templ": "html"
					}
				}
			}
		}
	},
	"terminal": {
		"env": {
			"EDITOR": "zed --wait"
		}
	},
	"node": {
		"path": "/home/g3ano/.nvm/versions/node/v22.15.1/bin/node",
		"npm_path": "/home/g3ano/.nvm/versions/node/v22.15.1/bin/npm"
	}
}
